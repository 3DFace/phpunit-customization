{
  "name": "dface/phpunit-customization",
  "description": "My custom comparison extension for phpunit",
  "keywords": [
    "phpunit",
    "comparison"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Ponomarev",
      "email": "ponomarev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "dface\\PhpunitCustomization\\": [
        "lib/"
      ]
    }
  },
  "config": {
    "platform": {
      "php": "8.2.5"
    }
  }
}
